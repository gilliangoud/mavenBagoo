<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="hoofdSchermService" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fortw.bagoo.service.HoofdSchermServiceController">
    <children>
      <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <center>
            <StackPane fx:id="contentPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <fx:include fx:id="vermissingPane" source="/com/fortw/bagoo/panes/VermissingPane.fxml" visible="true"/>
               </children>
            </StackPane>
         </center>
         <left>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
               <children>
                        <Button fx:id="klantzoeken" mnemonicParsing="false" onAction="#handleOpenKlantZoeken" prefHeight="50.0" prefWidth="160.0" text="Klant opzoeken" />
                        <Button fx:id="knopVermissingMelden" mnemonicParsing="false" onAction="#handleVermissingAction" prefHeight="50.0" prefWidth="160.0" text="vermissing melden" />
                        <Button fx:id="knnopOpenLogboek" mnemonicParsing="false" onAction="#handleOpenlogBoekAction" prefHeight="50.0" prefWidth="160.0" text="Logboek" />
                        <Button fx:id="knopClaim" mnemonicParsing="false" onAction="#handleClaimAction" prefHeight="50.0" prefWidth="160.0" text="Claim" />
               </children>
               <effect>
                  <SepiaTone level="0.0" />
               </effect>
            </VBox>
         </left>
         <bottom>
            <HBox fillHeight="false" maxWidth="1.7976931348623157E308" BorderPane.alignment="BOTTOM_LEFT">
               <children>
                        <Button fx:id="knopLoguit" alignment="CENTER" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#handleLoginAction" prefHeight="25.0" prefWidth="160.0" text="Log uit" HBox.hgrow="NEVER" />
               </children>
            </HBox>
         </bottom>
         <top>
            <HBox prefHeight="50.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
               <children>
                        <ImageView fitHeight="72.0" fitWidth="207.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                        <Image url="img/bagoo-logo.png" />
                            </image>
                        </ImageView>
               </children>
            </HBox>
         </top>
      </BorderPane>
    </children>
</AnchorPane>
