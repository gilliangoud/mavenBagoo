<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="klantenPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fortw.bagoo.panes.VermissingPaneController">
   <children>
      <BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="knopVerversKlant" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVerversAction" prefHeight="50.0" prefWidth="160.0" text="Ververs" />
                  <Button fx:id="knopBewerkKlant" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleBewerkAction" prefHeight="50.0" prefWidth="160.0" text="Bewerk klant" />
                  <Button fx:id="knopVerwijderKlant" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleVerwijderAction" prefHeight="50.0" prefWidth="160.0" text="Verwijder klant" />
                  <Button fx:id="knopNieuweKlant" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#handleNieuwAction" prefHeight="50.0" prefWidth="160.0" text="Nieuwe klant" />
               </children>
            </VBox>
         </left>
         <center>
            <StackPane fx:id="stackPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER" />
            <fx:include fx:id="anchorPaneBKlant" source="BewerkKlantPane.fxml" visible="false" />
            <fx:include fx:id="anchorPaneNKlant" source="NieuweKlantPane.fxml" visible="false" />
         </center>
      </BorderPane>
   </children>
</AnchorPane>
